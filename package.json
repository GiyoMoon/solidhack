{
  "name": "solid-site",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "scripts": {
    "build": "run-s format type:check build:vite build:postoptimize",
    "preview": "vite preview",
    "https-preview": "vite build && serve dist",
    "dev": "vite",
    "build:vite": "vite build",
    "build:postoptimize": "node ./postoptimize.mjs",
    "build:sitemap": "jiti scripts/generateSitemap.js",
    "type:check": "tsc --noEmit",
    "format": "prettier -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"*.{js,ts,mjs}\"",
    "deploy:dev": "wrangler publish --env development",
    "deploy:prod": "wrangler publish --env production",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@mdx-js/rollup": "^2.0.0-rc.1",
    "@solid-primitives/clipboard": "^1.2.7",
    "@solid-primitives/cookies-store": "^1.1.11",
    "@solid-primitives/countdown": "^1.0.9",
    "@solid-primitives/debounce": "^1.0.8",
    "@solid-primitives/event-listener": "^1.3.8",
    "@solid-primitives/i18n": "^1.0.8",
    "@solid-primitives/intersection-observer": "^1.2.0",
    "@solid-primitives/scroll": "^1.0.4",
    "@solid-primitives/throttle": "^1.0.8",
    "@solid.js/docs": "^1.0.15",
    "fuse.js": "^6.4.6",
    "monaco-editor": "^0.30.1",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.1",
    "remark-oembed": "^1.2.2",
    "scroll-behavior-polyfill": "^2.0.13",
    "solid-app-router": "^0.1.14",
    "solid-dismiss": "^1.0.22",
    "solid-heroicons": "^1.1.0",
    "solid-js": "^1.3.0-rc.1",
    "solid-jsx": "^0.2.2",
    "solid-markdown": "^1.1.0",
    "solid-meta": "^0.27.2",
    "solid-repl": "^0.18.0",
    "solid-social": "^0.5.0",
    "solid-transition-group": "^0.0.8",
    "solid-utils": "^0.8.1"
  },
  "devDependencies": {
    "@aminya/minijson": "^0.6.2",
    "@tailwindcss/typography": "^0.4.1",
    "@types/markdown-it": "^12.2.3",
    "autoprefixer": "^10.4.0",
    "dotenv": "^10.0.0",
    "fast-glob": "^3.2.7",
    "front-matter": "^4.0.2",
    "globby": "^12.0.2",
    "got": "^11.8.2",
    "htmlnano": "^1.1.1",
    "https-localhost": "^4.7.0",
    "jiti": "^1.12.9",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "shiki": "^0.9.12",
    "svgo": "^2.8.0",
    "tailwindcss": "^2.2.19",
    "tailwindcss-dir": "^4.0.0",
    "tailwindcss-rtl": "^0.8.0",
    "terser-config-atomic": "^0.1.1",
    "typescript": "^4.5.2",
    "vite": "^2.7.6",
    "vite-plugin-pwa": "^0.11.5",
    "vite-plugin-solid": "^2.1.2"
  },
  "version": "0.0.0"
}
